<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SDKBrowser.Common.ExamplesPage"
             xmlns:local="clr-namespace:SDKBrowser.Common">
  <ContentPage.Resources>
    <ResourceDictionary>
      <local:ExampleNameConverter x:Key="exampleNameConverter" />
      <DataTemplate x:Key="itemTemplate">
        <TextCell Text="{Binding Converter={StaticResource exampleNameConverter}}" />
      </DataTemplate>
    </ResourceDictionary>
  </ContentPage.Resources>
</ContentPage>
